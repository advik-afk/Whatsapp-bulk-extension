{
  "manifest_version": 3,
  "name": "WTF",
  "description": "WTF",
  "version": "3.0.1",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "icons/wtf16.png",
    "32": "icons/wtf32.png",
    "48": "icons/wtf48.png",
    "64": "icons/wtf64.png",
    "128": "icons/wtf128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/wtf16.png",
      "32": "icons/wtf32.png",
      "48": "icons/wtf48.png",
      "64": "icons/wtf64.png",
      "128": "icons/wtf128.png"
    },
    "default_title": "WTF",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://web.whatsapp.com/*"
      ],
      "run_at": "document_start",
      "js": [
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "js/wa-js.js"
      ],
      "matches": [
        "https://web.whatsapp.com/*"
      ]
    }
  ],
  "storage": {
    "managed_schema": "schema.json"
  }
}