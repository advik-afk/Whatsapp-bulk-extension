{
  "manifest_version": 2,
  "name": "WTF",
  "version": "2.1",
  "description": "WTF",
  "icons": {
    "16": "icons/wtf16.png",
    "32": "icons/wtf32.png",
    "48": "icons/wtf48.png",
    "64": "icons/wtf64.png",
    "128": "icons/wtf128.png"
  },
  "page_action": {
    "default_icon": {
      "16": "icons/wtf16.png",
      "32": "icons/wtf32.png",
      "48": "icons/wtf48.png",
      "64": "icons/wtf64.png",
      "128": "icons/wtf128.png"
    },
    "default_title": "WTF",
    "default_popup": "popup.html"
  },
  "author": "Marcos V.",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "storage": {
    "managed_schema": "schema.json"
  }
}